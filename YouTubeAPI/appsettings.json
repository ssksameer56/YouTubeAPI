{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "YouTube": {
    "Keys": "{API_KEY_HERE}",
    "SearchSize": 10,
    "Delay": 30,
    "CheckRecent": "true",
    "keywordToSearch": "cricket"
  },
  "QuartzYTJob": {
    "cronExpression": "0/30 * * * * ?"
  }
}

